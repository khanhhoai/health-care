  <div class="container">

      <h1 class="heading-1">INTERNAL MEDICINE</h1>
      <div class="sub-heading">
        
      </div>
    

    <div class="main">
      <div class="home">
        <div class="left">
          

          <h2 class="heading-2">
            {{internalmedicines.title}}
          </h2>
          <p class="home-img"><span>{{internalmedicines.createAt}}</span></p>
          <img src="{{internalmedicines.image}}" class="home-img" alt="Paper photo">
        </div>

        
      </div>
               
      <div>
        <p class="internalmedicine" data-internalmedicine="{{internalmedicines.detaildescription}}">{{internalmedicines.detaildescription}}</p>
      </div>
      <p><strong>{{internalmedicines.author}}</strong></p>
    </div>
  </div>

<script>
    // detaildescription
    const internalmedicines = document.querySelectorAll(".internalmedicine");
      
    // Loop through each element with the class "internalmedicine"
    internalmedicines.forEach((element) => {
    // Get the value of the data-presentation attribute
    const internalmedicine = element.dataset.internalmedicine;
        
    // Example: Manipulate the content of each element
    element.innerHTML = internalmedicine;
    return element.textContent || element.innerText || '';
    });
</script>
  




